<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  
  <appSettings>
    <add key="rabbit_host" value="localhost" />
  </appSettings>

  <connectionStrings>
    <add connectionString="tXxR7zUKZgJuPw7bcd27UKKHoZra8HFRsAIt1moYRirQumStZ/sYMCjNcgOEJLZkZiU1Azo+AMxMeBFpSZUYONY9Dk3f9r7eAvf9J415JvQ=" name="master" />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>


  <log4net>
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <file value="MassTransit.log" />
      <appendToFile value="true" />
      <maximumFileSize value="1000KB" />
      <maxSizeRollBackups value="5" />

      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level %thread %logger - %message%newline" />
      </layout>
    </appender>

    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFile" />
    </root>
  </log4net>
  
</configuration>